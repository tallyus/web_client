{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-decorators",
    "transform-object-rest-spread",
    "transform-runtime",
    "react-html-attrs"
  ],
  "env": {
      "node": {
        "plugins": [
          [
            "babel-plugin-transform-require-ignore",
            {
              "extensions": [".less", ".css"]
            }
          ]
        ]
      }
    }
}
