  "scripts": {
    "build": "cross-env NODE_ENV='production' webpack -p",
    "start": "cross-env NODE_ENV='production' BABEL_ENV='node' pm2 start server/runner.js --name itsquiz-wall",
    "nodemon": "cross-env BABEL_ENV='node' nodemon server/runner.js",
    "webpack-devserver": "webpack-dev-server --debug --hot --devtool eval-source-map --output-pathinfo --watch --progress --colors --inline --content-base public --port 8050 --host 0.0.0.0",
    "extract_lang": "./bin/extract_lang.sh",
    "lint": "eslint shared client server --ext js,jsx",
    "lint:fix": "eslint shared client server --ext js,jsx --fix",
    "test": "npm run lint",
    "po2json": "./bin/po2json.sh",
    "stop": "cross-env NODE_ENV='production' pm2 delete itsquiz-wall -s &> /dev/null",
    "old_1_start": "nodemon dist/src/server.js --exec babel-node",
    "old_1_build": "npm run clean && NODE_ENV=development PROCESS_PORT=3000 webpack src -d --progress --colors && NODE_ENV=development PROCESS_PORT=3000 babel src -d dist && mkdir -p ./dist/views/ && cp ./src/views/index.ejs ./dist/views/index.ejs",
    "old_1_serve": "NODE_ENV=development PROCESS_PORT=3000 node dist/server.js",
    "old_1_test": "mocha --compilers js:babel-register",
    "old_1_clean": "rm -rf ./dist/* ./babel_cache"
  },
